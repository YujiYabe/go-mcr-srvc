docker-compose stop
docker system prune

sudo rm -rf db/engine/mysql/var_lib_mysql/
sudo rm -rf db/engine/mysql/data/
sudo rm -rf db/tool/phpmyadmin/sessions/
sudo rm -rf go_src/go_api8/gin-bin



docker-compose build --no-cache

docker-compose up



