{
    "cSpell.import": [
        "./.vscode/cspell.json"
    ],
    "cSpell.ignorePaths": [
        "package-lock.json",
        "node_modules",
        "vscode-extension",
        ".git/{info,lfs,logs,refs,objects}/**",
        ".git/{index,*refs,*HEAD}",
        ".vscode",
        ".vscode-insiders",
        "settings.json"
    ],
    "makefile.configureOnOpen": false,
    "sqltools.connections": [
        {
            "previewLimit": 50,
            "server": "localhost",
            "port": 15432,
            "driver": "PostgreSQL",
            "name": "postgres local",
            "database": "app",
            "username": "user",
            "password": "user"
        }
    ],
}
